{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "logHost": "192.168.99.100",
  "amqpHost": "192.168.99.100",
  "amqpPort": 5672,
  "appType": "sandbox",
  "btv3": {
    "queueName": "btv3",
    "syncStoresJob": "sync-stores",
    "syncEANJob": "sync-ean",
    "stores": [
      {
        "name": "Unites States",
        "countryCode": "US",
        "awsHost": "webservices.amazon.com"
      },
      {
        "name": "United Kingdom",
        "countryCode": "UK",
        "awsHost": "webservices.amazon.co.uk"
      },
      {
        "name": "Germany",
        "countryCode": "DE",
        "awsHost": "webservices.amazon.de"
      },
      {
        "name": "Australia",
        "countryCode": "AU"
      },
      {
        "name": "Canada",
        "countryCode": "CA",
        "awsHost": "webservices.amazon.ca"
      }
    ]
  }
}
